{
  "name": "winery-erp-frontend",
  "version": "0.0.1",
  "description": "Winery ERP Frontend - Angular Application",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
